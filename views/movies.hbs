<div id="movies-container">
    <h2>Ironhack Cinema</h2>
    <h4>Click on the movie to check the showtimes!</h4>
<section>
    {{#each response}}
    <div id="movie">
    <img src="{{this.image}}" alt="{{this.title}}">
    <h3>{{this.title}}</h3>
    <a href="/movies/{{this._id}}">See more</a>
    </div>
    {{/each}}
</section>
  
</div>

